<%- include('./partials/head'); %>  
<title>Users</title> 
<style>
    body{
        background-color: rgb(11, 75, 53);
    }
   h1{
    text-align: center;
    color: aliceblue;
    /* text-decoration: underline; */
   }
   ul{
    width: 500px;
    background-color: aliceblue;
    font-size: 18px;
    list-style: none;
    padding: 2rem;
    margin: auto;
   }
   .usr{
    border: 1px solid gray;
    padding: 0.5rem;
    margin: 0.2rem 0;
   }
   
</style>
<body>
   <h1>Our users</h1>
   <ul>
     <% users.forEach(function(result) { %>
        <div class="usr">
         <li><%= result.name %></li>
        <li><%= result.email %></li>
        <li><%= result.password %></li>   
        </div>
    <%});%>
   </ul>
  
    
</body>
</html>